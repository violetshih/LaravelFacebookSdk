{
    "name": "violetshih/laravel-facebook-sdk",
    "description": "Fully unit tested Facebook SDK v5 integration for Laravel & Lumen 5.x",
    "keywords": ["facebook", "laravel", "lumen", "sdk", "facebook sdk v4", "facebook sdk v5"],
    "homepage": "https://github.com/violetshih/LaravelFacebookSdk",
    "license": "MIT",
    "authors": [
        {
            "name": "violetshih",
            "email": "swi2a@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        
        "facebook/graph-sdk": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0|~6.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "violetshih\\LaravelFacebookSdk\\": "src/LaravelFacebookSdk"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "violetshih\\LaravelFacebookSdk\\Test\\": "tests"
        }
    },
    "branch-alias": {
        "dev-master": "3.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "violetshih\\LaravelFacebookSdk\\LaravelFacebookSdkServiceProvider"
            ],
            "aliases": {
                "Facebook": "violetshih\\LaravelFacebookSdk\\FacebookFacade"
            }
        }
    }
}
